<h2>Book this dog!</h2>
<%= simple_form_for [@dog, @booking] do |f| %>
  <%= f.input :start_date,
              as: :string,
              input_html: { data: { controller: "datepicker" } } %>
  <%= f.input :end_date,
              as: :string,
              input_html: { data: { controller: "datepicker" } } %>
  <%= f.submit "Send request", class: "btn btn-primary booking-submit" %>
<% end %>
